    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "sass": "^1.89.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^3.2.4"
