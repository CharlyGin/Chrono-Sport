{
  "name": "chrono-sport",
  "version": "1.0.0",
  "description": "Web app for sport chronometers",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\"",
